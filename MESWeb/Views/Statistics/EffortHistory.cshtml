@model List<T_ResultData>

<div class="span12">
    <div class="widget-box">
        <div class="widget-title">
            <span class="icon"><i class="icon-th"></i></span>
            <h5>Search result: @Model.Count record(s)</h5>
        </div>
        <div class="widget-content nopadding">
            <table class="table table-bordered data-table">
                <thead>
                    <tr>
                        <th>
                            <input type="checkbox" id="chbAll" />
                        </th>
                        <th>Date</th>
                        <th>Cus_worker</th>
                        <th>Cus_lineno</th>
                        <th>Cus_processname</th>
                    </tr>
                </thead>
                <tbody>
                    @for (int i = 0; i < Model.Count; i++)
                    {
                        <tr class="odd gradeX">
                            <td style="text-align:center;">
                                <input type="checkbox" value="@Model[i].ID" id="chb@(Model[i].ID)" class="chbViewPlanChart" />
                            </td>
                            <td>
                                <span>@Model[i].Dv_date</span>
                            </td>
                            <td>
                                <span>@Model[i].Dv_machineid</span>
                            </td>
                            <td>
                                <span>@Model[i].Dv_worker</span>
                            </td>
                            <td>
                                <span>@Model[i].Dv_lineno</span>
                            </td>
                            <td>
                                <span>@Model[i].Dv_processname</span>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

